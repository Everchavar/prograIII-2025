      "cell_type": "code",
      "source": [
        "def acumulacion_meses_por_mes(n):\n",
        "    # use la f√≥rmula triangular\n",
        "    return [(mes, (mes * (mes + 1)) // 2) for mes in range(1, n + 1)]\n",
        "\n",
        "def mostrar_tabla(tabla):\n",
        "    print(\"\\nTabla de acumulaci√≥n de meses:\\n\")\n",
        "    print(f\"{'Mes':<6} | {'Acumulado':<10}\")\n",
        "    print(\"-\" * 22)\n",
        "    for mes, acumulado in tabla:\n",
        "        print(f\"{mes:<6} | {acumulado:<10}\")\n",
        "    print(\"-\" * 22)\n",
        "\n",
        "def iniciar():\n",
        "    print(\"Calculador de Meses Acumulados\\n\")\n",
        "    try:\n",
        "        n = int(input(\"Ingrese el n√∫mero de meses (n): \"))\n",
        "        if n <= 0:\n",
        "            print(\"Por favor ingrese un n√∫mero positivo.\")\n",
        "            return\n",
        "        tabla = acumulacion_meses_por_mes(n)\n",
        "        mostrar_tabla(tabla)\n",
        "        total = (n * (n + 1) * (n + 2)) // 6\n",
        "        print(f\"\\nAcumulaci√≥n total hasta el mes {n}: {total} meses\\n\")\n",
        "    except ValueError:\n",
        "        print(\"Entrada inv√°lida. Por favor ingrese un n√∫mero entero.\")\n",
        "\n",
        "iniciar()"
      ],
      "metadata": {
        "id": "SbOpaKQ_KqJA"
      },
      "execution_count": null,
      "outputs": []
